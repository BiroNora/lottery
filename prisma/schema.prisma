generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//datasource db {
//  provider = "postgresql"
//  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
//  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
//}

model Five {
  five_id  Int @id @default(autoincrement())
  f_year  Int
  f_week Int
  f_total  BigInt
  f_first  Int
  f_second Int
  f_third  Int
  f_fourth  Int
  f_fifth  Int

  @@map("five")
}

model Six {
  six_id   Int @id @default(autoincrement())
  s_year Int
  s_week Int
  s_total  BigInt
  s_first  Int
  s_second Int
  s_third  Int
  s_fourth  Int
  s_fifth  Int
  s_sixth  Int

  @@map("six")
}

model Skandi {
  sk_id   Int @id @default(autoincrement())
  sk_year Int
  sk_week Int
  sk_total  BigInt
  m_first  Int
  m_second Int
  m_third  Int
  m_fourth  Int
  m_fifth  Int
  m_sixth  Int
  m_seventh Int
  h_first  Int
  h_second Int
  h_third  Int
  h_fourth  Int
  h_fifth  Int
  h_sixth  Int
  h_seventh Int

  @@map("skandi")
}
